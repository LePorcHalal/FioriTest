<mvc:View controllerName="com.beyondtechnologies.controller.CreateProject" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form"
	xmlns:footerbar="sap.ushell.ui.footerbar" xmlns:l="sap.ui.layout" xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.m.semantic"
	xmlns:smart="sap.ui.comp.smartfield" xmlns="sap.m">
	<semantic:DetailPage id="page" navButtonPress="onNavBack" title="New Effort" busy="{viewModel>/busy}">
		
		<semantic:customHeaderContent>
			
		</semantic:customHeaderContent>
		<semantic:content>
			
			<f:SimpleForm class="editableForm" columnsL="1" columnsM="1" editable="true" emptySpanL="4" emptySpanM="4" id="newEntitySimpleForm"
				labelSpanL="3" labelSpanM="3" layout="ResponsiveGridLayout" maxContainerCols="2" minWidth="1024"
				title="{= ${viewModel>/mode} === 'edit'? ${i18n>editProjectTitle} : ${i18n>createProjectTitle} }">
				
				
				<f:content>
					<!--<Input name="ID" enabled="true" visible="false" value="0"/>-->
					
						
			<!--	<Label text="Product" labelFor="productInput"/>-->
			<!--<Input-->
			<!--	id="productInput"-->
			<!--	type="Text"-->
			<!--	placeholder="Enter Product ..."-->
			<!--	showSuggestion="true"-->
			<!--	showValueHelp="true"-->
			<!--	valueHelpRequest="handleValueHelp"-->
			<!--	suggestionItems="{/FRICE}" >-->
			<!--	<suggestionItems>-->
			<!--		<core:Item text="{Name}" />-->
			<!--	</suggestionItems>-->
			<!--</Input>-->
			
			
			
				<Label text="Id" required="true"/>
					<Input name="ID" id="ID_id" valueLiveUpdate="true" liveChange="_validateSaveEnablement" enabled="true" visible="true"
						value="{ path: 'ID', type: 'sap.ui.model.odata.type.String'}"/>
					
						
				<Label text="{i18n>ECOMPLEXITYTitle}" required="true"/>
				<Select
					forceSelection="false"
					selectedKey="{COMPLEXITY}">
					
					<core:Item key="1-Very Low" text="1-Very Low"/>
					<core:Item key="2-Low" text="2-Low"/>
					<core:Item key="3-Medium" text="3-Medium"/>
					<core:Item key="4-High" text="4-High"/>
					<core:Item key="5-Very High" text="5-Very High"/>
				</Select>
					
					
					
					<Label text="{i18n>FSTitle}" required="true"/>
					<Input name="FS" id="FS_id" valueLiveUpdate="true" liveChange="_updateFUNC" enabled="true" visible="true"
						value="{ path: 'FS', type: 'sap.ui.model.odata.type.Decimal'}"/>
					
					<Label text="{i18n>EFRICETitle}" required="true"/>
					<Input name="FRICE" id="FRICE_id" valueLiveUpdate="true" liveChange="_validateSaveEnablement" enabled="true" visible="true"
						value="{ path: 'FRICE', type: 'sap.ui.model.odata.type.String' }"/>
				 	
					<Label text="{i18n>FS_SUPTitle}" required="true"/>
					<Input name="FS_SUP" id="FS_SUP_id" valueLiveUpdate="true" liveChange="_updateTECH" enabled="true" visible="true"
						value="{ path: 'FS_SUP', type: 'sap.ui.model.odata.type.Decimal'}"/>
					
					<Label text="{i18n>FS_REVTitle}" required="true"/>
					<Input name="FS_REV" id="FS_REV_id" valueLiveUpdate="true" liveChange="_updateTECH" enabled="true" visible="true"
						value="{ path: 'FS_REV', type: 'sap.ui.model.odata.type.Decimal' }"/>
					
					<Label text="{i18n>TSTitle}" required="true"/>
					<Input name="TS" id="TS_id" valueLiveUpdate="true" liveChange="_updateTECH" enabled="true" visible="true"
						value="{ path: 'TS', type: 'sap.ui.model.odata.type.Decimal' }"/>
					
					<Label text="{i18n>DEV_UTTitle}" required="true"/>
					<Input name="DEV_UT" id="DEV_UT_id" valueLiveUpdate="true" liveChange="_updateTECH" enabled="true" visible="true"
						value="{ path: 'DEV_UT',type: 'sap.ui.model.odata.type.Decimal' }"/>
					
						<Label text="{i18n>DEV_SUPTitle}" required="true"/>
					<Input name="DEV_SUP" id="DEV_SUP_id" valueLiveUpdate="true" liveChange="_updateFUNC" enabled="true" visible="true"
					value="{ path: 'DEV_SUP', type: 'sap.ui.model.odata.type.Decimal' }"/>
					
						<Label text="{i18n>FUTTitle}" required="true"/>
					<Input name="FUT" id="FUT_id" valueLiveUpdate="true" liveChange="_updateFUNC" enabled="true" visible="true"
					value="{ path: 'FUT', type: 'sap.ui.model.odata.type.Decimal' }"/>
					
					<Label text="{i18n>FUT_SUPTitle}" required="true"/>
					<Input name="FUT_SUP" id="FUT_SUP_id" valueLiveUpdate="true" liveChange="_updateTECH" enabled="true" visible="true"
					value="{ path: 'FUT_SUP', type: 'sap.ui.model.odata.type.Decimal' }"/>
					
					<Label text="{i18n>FIT_SUPTitle}" required="true"/>
					<Input name="FIT_SUP" id="FIT_SUP_id" valueLiveUpdate="true" liveChange="_updateTECH" enabled="true" visible="true"
					value="{ path: 'FIT_SUP', type: 'sap.ui.model.odata.type.Decimal' }"/>
					
					<Label text="{i18n>FUNC}" required="false"/>
					<Input name="FUNC" id="FUNC_id" valueLiveUpdate="true"  enabled="false" visible="true"
					value="{ path: 'FUNC', type: 'sap.ui.model.odata.type.Decimal' }"/>
					
					<Label text="{i18n>TECH}" required="false"/>
					<Input name="TECH" id="TECH_id" valueLiveUpdate="true"  enabled="false" visible="true" 
					value="{ path: 'TECH', type: 'sap.ui.model.odata.type.Decimal' }"/>
					
					<Label text="{i18n>TECH_ARCHTitle}" required="true"/>
					<Input name="TECH_ARCH" id="TECH_ARCH_id" valueLiveUpdate="true" liveChange="_updateGrandTotal" enabled="true" visible="true" 
					value="{ path: 'TECH_ARCH', type: 'sap.ui.model.odata.type.Decimal' }"/>
					
					<Label text="{i18n>TECH_LEADTitle}" required="true"/>
					<Input name="TECH_LEAD" id="TECH_LEAD_id" valueLiveUpdate="true" enabled="true" liveChange="_updateGrandTotal" visible="true"
					value="{ path: 'TECH_LEAD', type: 'sap.ui.model.odata.type.Decimal' }"/>
					
					<Label text="{i18n>GRAND_TOTALTitle}" required="false"/>
					<Input name="GRAND_TOTAL" id="GRAND_TOTAL_id" valueLiveUpdate="true" enabled="false" visible="true"
					value="{ path: 'GRAND_TOTAL', type: 'sap.ui.model.odata.type.Decimal' }"/>
					
				</f:content>
			</f:SimpleForm>
		</semantic:content>
		<semantic:saveAction>
			<semantic:SaveAction id="save" enabled="{viewModel>/enableCreate}" press="checkIfID"/>
		</semantic:saveAction>
		<semantic:cancelAction>
			<semantic:CancelAction id="cancel" press="onCancel"></semantic:CancelAction>
		</semantic:cancelAction>
	</semantic:DetailPage>
</mvc:View>